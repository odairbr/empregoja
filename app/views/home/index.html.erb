<% @jobs.each do |job| %>
  <div>
    <p>
      <h1> <%= job.title %> </h1>

      <% if job.featured? %>
        <strong> Destaque </strong>
      <% end %>
      <%= job.category %><br>
      <%= job.company.name %><br>

      <%= job.location %><br>
      <%= link_to job.title, job %>


    </p>
  </div>

  <% end %>
